<UserControl x:Class="PAT_Editor.ucTS"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PAT_Editor"
             mc:Ignorable="d" 
             d:DesignHeight="60" d:DesignWidth="60">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" Name="lblTS" Text="TS" VerticalAlignment="Bottom" HorizontalAlignment="Center"></TextBlock>
        <TextBox Grid.Row="1" Name="txtTS" Width="50" VerticalAlignment="Top" ToolTip="1/(5ns * X) MHz"
                 HorizontalAlignment="Center" HorizontalContentAlignment="Center" Margin="5"></TextBox>
    </Grid>
</UserControl>
