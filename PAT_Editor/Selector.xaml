<Window x:Class="PAT_Editor.Selector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PAT_Editor"
        mc:Ignorable="d"
        Title="Selector" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Name="btnPhase1" Content="Phase 1" Margin="30" Click="btnPhase1_Click"></Button>
        <Button Grid.Row="1" Name="btnPhase2" Content="Phase 2" Margin="30" Click="btnPhase2_Click"></Button>
    </Grid>
</Window>
